class Person:
    def __init__(self, gender):
        self.gender = gender

    def getGender(self):
        return self.gender

class Male(Person):
    def __init__(self):
        super().__init__("Male")

class Female(Person):
    def __init__(self):
        super().__init__("Female")

male = Male()
female = Female()

print(male.getGender())   
print(female.getGender()) 
